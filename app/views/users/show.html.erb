    <section class="user_info">
      <h1>
        <%= @user.name %><br>
        <%= @user.email %><br>
        <%= @user.height %><br>
        <%= @user.weight %><br>
        <%= @user.sex %><br>
        <%= @user.age %><br>
        <%= @user.purpose %><br>
        <%= @user.goal_weight %><br>
        <%= @user.activity_level %>
      </h1>
    </section>
  </aside>
</div>
